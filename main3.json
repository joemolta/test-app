{
    "$jason": {
        "head": {
            "title": "Test App",
            "description": "This an example of an attempt at a functional app",
            "actions": {
                "$load": {
                    "type" : "$render",
                    "success": {
                        "type": "$agent.inject",
                            "options" : {
                                "id" : "$webcontainer",
                                "items" : [
                                    {
                                        "url" : "https://code.jquery.com/jquery-3.4.1.min.js"
                                    },
                                    {
                                        "url" : "CHANGE_ME_WITH_VALID_URL/fixes.js"
                                    }
                                ]
                            }
                    }
                },
                "linkcatcher" : {
                    "type" : "$jason.alert",
                    "options" : {
                        "title" : "Link Clicked",
                        "description" : "Jason {{$jason.url}} clicked"
                    } 
                },
                "handleURL":{
                    "type": "$util.url",
                    "options":{
                        "title":"Link Clicked",
                        "description":"{{$jason.url}}"
                    }
                }
            },
            "templates": {
                "body": {
                    "header": {
                        "title": "Test",
                        "style": {
                            "background" : "#4976BA"
                        }
                    },
                    "background": {
                        "type": "html",
                        "url": "https://github.com",
                        "action": {
                            "trigger": "handleURL"
                        }
                    }
                }
            }
        }
    }
}