{
    "$jason": {
        "head": {
            "title": "Test App",
            "description": "This an example of an attempt at a functional app",
            "actions": {
                "$load": {
                    "type" : "$render",
                    "success": {
                        "type": "$agent.inject",
                            "options" : {
                                "id" : "$webcontainer",
                                "items" : [
                                    {
                                        "url" : "https://code.jquery.com/jquery-3.4.1.min.js"
                                    }
                                ]
                            }
                    }
                }
            },
            "templates": {
                "body": {
                    "header": {
                        "title": "Test App",
                        "style": {
                            "background" : "#4976BA"
                        }
                    },
                    "background": {
                        "type": "html",
                        "url": "https://google.com",
                        "action": {
                            "trigger": "linkcatcher"
                        }
                    }
                }
            }
        }
    }
}