{
    "$jason": {
        "head": [{
            "title": "Test App",
            "actions": {
                "$foreground": {
                    "type": "$reload"
                },//Goal of $foreground $reload is to refresh the web page being injected
	            "$load": {
	                "type" : "$render",
	                "success": {
	                    "type": "$agent.inject",
	                        "options" : {
	                            "id" : "$webcontainer",
	                            "items" : [{
	                                "url" : "https://code.jquery.com/jquery-3.4.1.min.js"
	                            }]
	                        }
	                }
	            }
            }//end actions
        }],//end head
        "body":{
            "header": {
                "type":"label",
                "text": "Test App"
            },//end body.header
            "background": [{
                "type": "html",
                "url": "https://www.google.com",
                "action":{
                    "trigger": "navigate"
                }
            }],//end background.footer
            "footer":{
                "tabs":{
                    "style":{
                      "background":"#ffff00"
                    },
                    "items":[{
                        "type":"label",
                        "text":"A"
                    },{
                        "type":"label",
                        "text":"B"
                    },{
                        "type":"label",
                        "text":"C"
                    }]//end body.footer.tabs.items
                }//end body.footer.tobs
            }//end body.footer
        }//end body
    }//end $jason
}
